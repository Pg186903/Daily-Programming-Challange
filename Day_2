#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cout<<"Enter the size of the array : "<<endl;
    cin>>n;
    cout<<"Enter array elements : "<<endl;
    int sum = n*(n+1)/2;
    int temp,sum1 = 0;
    for(int i = 1;i<n;i++)
    {
        cin>>temp;
        sum1 += temp;
    }
    cout<<"The missing number is : "<<sum-sum1<<endl;
}
