#include<bits/stdc++.h>
using namespace std;

vector<int> kadane(vector<int> a)
{
    int l,r,m;
    l = m = 0;
    r = a.size() - 1;
    while(m<r)
    {
        if(a[m]==1)
        {
            m++;
        }
        else if(a[m]==2)
        {
            a[m] = a[r];
            a[r] = 2;
            r--;
        }
        else
        {
            a[m] = a[l];
            a[l] = 0;
            l++;
            m++;
        }
    }
    return a;
}

int main()
{
    vector<int> input,ans;
    int n,temp;
    cout<<"Enter size of the array : "<<endl;
    cin>>n;
    cout<<"Enter the array elements : "<<endl;
    for(int i = 0;i<n;i++)
    {
        cin>>temp;
        input.push_back(temp);
    }
    ans = kadane(input);
    for(int i = 0;i<n;i++)
    {
        cout<<ans[i]<<" ";
    }
}
